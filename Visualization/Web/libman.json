{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "font-awesome@5.8.1",
      "destination": "wwwroot/lib/font-awesome/",
      "files": [
        "css/all.min.css"
      ]
    },
    {
      "library": "p5.js@0.8.0",
      "destination": "wwwroot/lib/p5.js/",
      "files": [
        "p5.min.js",
        "addons/p5.dom.min.js"
      ]
    },
    {
      "library": "apexcharts@3.6.8",
      "destination": "wwwroot/lib/apexcharts/"
    },
    {
      "library": "mappa-mundi@0.0.5",
      "destination": "wwwroot/lib/mappa",
      "provider": "unpkg"
    }
  ]
}